{
    "name": "starex-client-dashboard",
    "version": "1.0.0",
    "description": "Client Dashboard",
    "scripts": {
        "start:server": "cd ./server && npm run-script serve:watch",
        "start": "concurrently --kill-others \" webpack-cli serve --mode development --host 0.0.0.0 \" \" npm run start:server \"",
        "build": "webpack --mode production",
        "test": "jest --detectOpenHandles --forceExit --coverage --runInBand --config jest.config.json",
        "test:watch": "jest --watchAll --config jest.config.json",
        "lint": "eslint '**/*.{ts,tsx}'",
        "lint:fix": "eslint '**/*.{ts,tsx}' --fix",
        "format": "prettier --write '**/*.{ts,tsx,scss,css,js,jsx,json}'",
        "format-prettier": "prettier --eslint-config-path .eslintrc.js --write '**/*.{ts,tsx,scss,css,js,jsx,json}'"
    },
    "author": "mahabbatmh",
    "license": "ISC",
    "dependencies": {
        "axios": "0.21.1",
        "bootstrap": "^4.6.0",
        "history": "4.10.1",
        "js-cookie": "^2.2.1",
        "lodash": "4.17.20",
        "moment": "^2.29.1",
        "react": "16.14.0",
        "react-circular-progressbar": "^2.0.4",
        "react-context-devtool": "^2.0.3",
        "react-dom": "16.14.0",
        "react-error-boundary": "^3.1.3",
        "react-loading": "^2.0.3",
        "react-paginate": "^7.1.3",
        "react-range-picker": "^1.0.1",
